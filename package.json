{
  "name": "travel-note",
  "version": "1.0.0",
  "private": true,
  "description": "travel-note",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "Sass",
    "framework": "React"
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "stylelint": "stylelint **/*.{css,scss,less,html,vue}",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:harmony-hybrid": "taro build --type harmony-hybrid",
    "dev:weapp": "npm run build:weapp -- --watch --port 8080",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:harmony-hybrid": "npm run build:harmony-hybrid -- --watch",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.24.4",
    "@tarojs/components": "4.0.12",
    "@tarojs/helper": "4.0.12",
    "@tarojs/plugin-framework-react": "4.0.12",
    "@tarojs/plugin-platform-alipay": "4.0.12",
    "@tarojs/plugin-platform-h5": "4.0.12",
    "@tarojs/plugin-platform-harmony-hybrid": "4.0.12",
    "@tarojs/plugin-platform-jd": "4.0.12",
    "@tarojs/plugin-platform-qq": "4.0.12",
    "@tarojs/plugin-platform-swan": "4.0.12",
    "@tarojs/plugin-platform-tt": "4.0.12",
    "@tarojs/plugin-platform-weapp": "4.0.12",
    "@tarojs/react": "4.0.12",
    "@tarojs/runtime": "4.0.12",
    "@tarojs/shared": "4.0.12",
    "@tarojs/taro": "4.0.12",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "taro-ui": "^3.3.0",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-transform-class-properties": "7.25.9",
    "@babel/preset-react": "^7.24.1",
    "@commitlint/config-conventional": "^19.8.0",
    "@tarojs/cli": "4.0.12",
    "@tarojs/vite-runner": "4.0.12",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^3.1.2",
    "babel-preset-taro": "4.0.12",
    "commitlint": "^19.8.0",
    "cz-git": "^1.11.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-config-taro": "4.0.12",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.1",
    "postcss": "^8.4.38",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.3",
    "react-refresh": "^0.14.0",
    "sass": "^1.75.0",
    "stylelint": "^16.4.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "terser": "^5.30.4",
    "typescript": "^5.4.5",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,less}": [
      "stylelint --fix",
      "prettier --write"
    ]
  }
}
